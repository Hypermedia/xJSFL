  "value": [
    "/**", 
    " * Saves and launches the current file through the xJSFL panel", 
    " * @version 2.0", 
    " */", 
    "\t", 
    "// -------------------------------------------------------------------------------------------------", 
    "// functions", 
    "// -------------------------------------------------------------------------------------------------", 
    "", 
    "\t/**", 
    "\t * Main function", 
    "\t */", 
    "\tfunction main()", 
    "\t{", 
    "\t\t// variables", 
    "\t\t\tvar view\t= ko.views.manager.currentView;", 
    "\t\t\tvar saved\t= xjsfl.save(view)", 
    "\t\t\t", 
    "\t\t// variables", 
    "\t\t\tif(saved)", 
    "\t\t\t{", 
    "\t\t\t\txjsfl.launch(view);", 
    "\t\t\t}\t\t\t", 
    "\t}", 
    "", 
    "// -------------------------------------------------------------------------------------------------", 
    "// code", 
    "// -------------------------------------------------------------------------------------------------", 
    "", 
    "\tmain();"
  ], 
